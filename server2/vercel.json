{
    "version": 2,
    "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "public/dist"
      }
    }
  ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "src/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/dist/$1"
        }
    ]
}